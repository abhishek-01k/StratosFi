# Private key for deployment and testing (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URLs for different networks
# Polygon (recommended for cheap testing)
POLYGON_RPC_URL=https://polygon-rpc.com
# or use a provider like Alchemy/Infura:
# POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/your-api-key

# Arbitrum (also cheap)
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
# or use a provider:
# ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/your-api-key

# Other networks
MAINNET_RPC_URL=https://eth.llamarpc.com
OPTIMISM_RPC_URL=https://mainnet.optimism.io

# Block Explorer API Keys (optional, for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key
ARBISCAN_API_KEY=your_arbiscan_api_key
POLYGONSCAN_API_KEY=your_polygonscan_api_key
OPTIMISTIC_ETHERSCAN_API_KEY=your_optimistic_etherscan_api_key

# 1inch Limit Order Protocol Addresses (V4)
# All chains except zkSync use the same address
ONEINCH_LIMIT_ORDER_PROTOCOL=0x111111125421ca6dc452d289314280a0f8842a65
# zkSync Era uses a different address
ONEINCH_LIMIT_ORDER_PROTOCOL_ZKSYNC=0x6fd4383cb451173d5f9304f041c7bcbf27d561ff

# Test Configuration
TEST_CHAIN=polygon  # Options: polygon, arbitrum, optimism, etc.
TEST_AMOUNT=2000000000000000000  # 2 MATIC/ETH in wei (adjust based on chain)

# Oracle Configuration (optional)
