{
  "name": "1inch-fusion-tee-solver",
  "version": "0.0.1",
  "description": "1inch Fusion+ TEE Solver using NEAR's Shade Agent Framework",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node -r tsconfig-paths/register -r ts-node/register src/main.ts",
    "dev": "node -r tsconfig-paths/register -r ts-node/register --watch src/main.ts",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "build": "tsc",
    "docker:build": "docker build -t 1inch-fusion-tee-solver .",
    "register": "node -r tsconfig-paths/register -r ts-node/register src/scripts/register-solver.ts"
  },
  "keywords": ["1inch", "fusion", "solver", "near", "tee", "shade-agent"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@1inch/fusion-sdk": "^2.3.6",
    "axios": "^1.7.0",
    "big.js": "^6.2.2",
    "borsh": "^2.0.0",
    "borsher": "^3.5.0",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.0",
    "joi": "^17.13.3",
    "near-api-js": "^4.0.3",
    "node-cache": "^5.1.2",
    "winston": "^3.3.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/big.js": "^6.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.1.0",
    "@types/ws": "^8.5.11",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^2.3.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "engines": {
    "node": ">=20.18.0"
  }
}